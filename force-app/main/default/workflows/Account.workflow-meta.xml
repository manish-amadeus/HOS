    <rules>
        <fullName>New News on Account</fullName>
        <actions>
            <name>AlertAccountOwnerwhennewnewshasbeenpostedtoanaccount</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Account.News_Date__c</field>
            <operation>equals</operation>
            <value>TODAY</value>
        </criteriaItems>
        <description>Alert the account owner that a new news item has been posted to one of their accounts.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Re-Sync GFT Fields to SAP</fullName>
        <actions>
            <name>Re_Sync_to_SAP_TRUE</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>NICC-056212 - Related to GFT SAP Integration</description>
        <formula>AND(OR( ISCHANGED( GFT_AccountName__c ), ISCHANGED( GFT_Address__c ), ISCHANGED( GFT_City__c ), ISCHANGED( GFT_Country__c ), ISCHANGED( GFT_PostalCode__c ), ISCHANGED( GFT_State__c ), ISCHANGED( c2g__CODACreditManager__c )), NOT(ISBLANK( GFT_AccountName__c )), NOT(ISBLANK( GFT_Address__c  )), NOT(ISBLANK( GFT_City__c )), NOT(ISBLANK( GFT_Country__c )), NOT(ISBLANK( c2g__CODACreditManager__c )))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
