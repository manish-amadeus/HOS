<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>AHP_Opportunities_Share</fullName>
        <accessLevel>Read</accessLevel>
        <description>Share AHP Opportunities with the Enterprise Sales Team Group</description>
        <label>AHP Opportunities Share</label>
        <sharedTo>
            <group>Enterprise_Sales_Team_Partner_Portal</group>
        </sharedTo>
        <criteriaItems>
            <field>RecordTypeId</field>
            <operation>equals</operation>
            <value>AHP Opportunity</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>false</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
    <sharingCriteriaRules>
        <fullName>Finance_Opportunity_Share</fullName>
        <accessLevel>Edit</accessLevel>
        <description>Grant members of the Finance role read/write access to opportunities that have the &quot;Sales Order&quot; record type</description>
        <label>Finance Opportunity Share</label>
        <sharedTo>
            <role>Finance</role>
        </sharedTo>
        <criteriaItems>
            <field>RecordTypeId</field>
            <operation>equals</operation>
            <value>Sales Order</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>false</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
    <sharingOwnerRules>
        <fullName>APExecutiveManagementA</fullName>
        <accessLevel>Edit</accessLevel>
        <label>AP - Executive ManagementA</label>
        <sharedTo>
            <role>APExecutiveManagement</role>
        </sharedTo>
        <sharedFrom>
            <roleAndSubordinates>APExecutiveManagement</roleAndSubordinates>
        </sharedFrom>
    </sharingOwnerRules>
    <sharingOwnerRules>
        <fullName>AllInternalUsers</fullName>
        <accessLevel>Edit</accessLevel>
        <label>AllInternalUsers</label>
        <sharedTo>
            <allInternalUsers></allInternalUsers>
        </sharedTo>
        <sharedFrom>
            <allPartnerUsers></allPartnerUsers>
        </sharedFrom>
    </sharingOwnerRules>
    <sharingOwnerRules>
        <fullName>CSD_Opportunities_Shared_with_EPM</fullName>
        <accessLevel>Edit</accessLevel>
        <label>CSD Opportunities Shared with EPM</label>
        <sharedTo>
            <role>EnterpriseProgramManagement</role>
        </sharedTo>
        <sharedFrom>
            <role>USCSD</role>
        </sharedFrom>
    </sharingOwnerRules>
    <sharingOwnerRules>
        <fullName>Operations</fullName>
        <accessLevel>Edit</accessLevel>
        <label>Operations</label>
        <sharedTo>
            <group>Operations</group>
        </sharedTo>
        <sharedFrom>
            <roleAndSubordinates>WWSales</roleAndSubordinates>
        </sharedFrom>
    </sharingOwnerRules>
    <sharingOwnerRules>
        <fullName>Support_Managers</fullName>
        <accessLevel>Read</accessLevel>
        <label>Support Managers</label>
        <sharedTo>
            <role>SupportManagers</role>
        </sharedTo>
        <sharedFrom>
            <roleAndSubordinates>WWSales</roleAndSubordinates>
        </sharedFrom>
    </sharingOwnerRules>
    <sharingOwnerRules>
        <fullName>USCSDManagementA</fullName>
        <accessLevel>Edit</accessLevel>
        <label>US - CSD ManagementA</label>
        <sharedTo>
            <role>USCSDManagement</role>
        </sharedTo>
        <sharedFrom>
            <roleAndSubordinates>USCSDManagement</roleAndSubordinates>
        </sharedFrom>
    </sharingOwnerRules>
    <sharingOwnerRules>
        <fullName>USDirectorofSalesA</fullName>
        <accessLevel>Edit</accessLevel>
        <label>US - Director of SalesA</label>
        <sharedTo>
            <role>USDirectorofSales</role>
        </sharedTo>
        <sharedFrom>
            <roleAndSubordinates>USDirectorofSales</roleAndSubordinates>
        </sharedFrom>
    </sharingOwnerRules>
</SharingRules>
