    <description>Updates all child case status</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND (
    [Case].RecordType.Name = &quot;Case New&quot;, 
    ISCHANGED([Case].Status),     
    OR (
        CONTAINS(TEXT([Case].Status), &quot;L3&quot;), 
        TEXT([Case].Status) = &quot;Platform Escalated&quot;
    ),
    NOT (TEXT([Case].Status) = &quot;L3 - Pending Customer Confirmation&quot;)
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND (
    {!myVariable_current.RecordType.Name} = &quot;Case New&quot;, 
    ISCHANGED({!myVariable_current.Status}),     
