<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>55.0</apiVersion>
    <description>NICC-061167; Checks the “IsOverridden__c” checkbox when any manual updates are made and the checkbox is not already checked.</description>
    <environments>Default</environments>
    <interviewLabel>AH Case Report Detail - Case Report Detail Overridden {!$Flow.CurrentDateTime}</interviewLabel>
    <label>AH Case Report Detail - Case Report Detail Overridden</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>FREE_FORM_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <description>Checks the &quot;Overridden&quot; checkbox to TRUE</description>
        <name>UpdateFieldValue</name>
        <label>UpdateFieldValue</label>
        <locationX>571</locationX>
        <locationY>197</locationY>
        <inputAssignments>
            <field>IsOverridden__c</field>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <start>
        <locationX>105</locationX>
        <locationY>39</locationY>
        <connector>
            <targetReference>UpdateFieldValue</targetReference>
        </connector>
        <filterFormula>{!$Record.IsOverridden__c} = FALSE</filterFormula>
        <object>AH_Case_Report_Detail__c</object>
        <recordTriggerType>Update</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Active</status>
</Flow>
