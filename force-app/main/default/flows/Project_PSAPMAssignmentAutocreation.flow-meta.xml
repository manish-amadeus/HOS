    <formulas>
        <description>Formula to populate the End Date on the &quot;End Date&quot; field on the Schedule record when creating a new Assignment</description>
        <name>formulaScheduleEndDate</name>
        <dataType>Date</dataType>
        <expression>IF(NOT(ISBLANK({!$Record.pse__End_Date__c})), 
{!$Record.pse__End_Date__c}, 
DATE(1900,01,01)
)</expression>
    </formulas>
