<apex:page standardController="User" recordSetVar="FrozenUsers" extensions="NI_FrozenUsers_ListView">
<apex:pageMessages id="idMessages" ></apex:pageMessages> 
    <apex:pageBlock title="Frozen Users">
        <apex:form id="theForm">
            <apex:pageBlockSection columns="1">
                <apex:pageBlockTable value="{!NI_FrozenUsers_ListView}" var="usr" >
                 <apex:column >
                     <apex:facet name="header">Full Name</apex:facet>
                         <apex:outputLink value="/{!usr.Id}?noredirect=1"> {!usr.Name}
                         </apex:outputLink>
                 </apex:column>
                 <apex:column >
                     <apex:facet name="header">User Type</apex:facet>
                         <apex:outputField value="{!usr.UserType}"/>
                 </apex:column>
                 <apex:column >
                     <apex:facet name="header">IsActive</apex:facet>
                         <apex:outputField value="{!usr.IsActive}"/>
                 </apex:column>
                 <apex:column >
                     <apex:facet name="header">UserName</apex:facet>
                         <apex:outputField value="{!usr.UserName}"/>
                 </apex:column>
                 <apex:column >
                     <apex:facet name="header">Last Login Date</apex:facet>
                         <apex:outputField value="{!usr.LastLoginDate}"/>
                 </apex:column>
                 <apex:column >
                     <apex:facet name="header">Profile Name</apex:facet>
                         <apex:outputField value="{!usr.Profile.Name}"/>
                 </apex:column>
                 <apex:column >
                     <apex:facet name="header">Xactly User</apex:facet>
                         <apex:outputField value="{!usr.Xactly_User__c}"/>
                 </apex:column>
                </apex:pageBlockTable>
            </apex:pageBlockSection>
            
        </apex:form> 
    </apex:pageBlock>
</apex:page>