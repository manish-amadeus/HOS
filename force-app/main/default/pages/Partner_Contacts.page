<apex:page controller="NI_PartnerContacts" sidebar="false" standardStylesheets="true" showheader="false">

<apex:stylesheet value="{!$Resource.PartnerCSS}" />
<apex:pageBlock >
 
 <table class="partnerContacts" >
 <apex:repeat value="{!partnerContacts}" var="p">
 
 
 <tr>
 <td rowspan="2">
     <apex:image value="{!p.FullPhotoURL}" />
 </td>
 </tr>
 
 
 

 
 <tr>
 <td>
      <apex:OutputText value="{!p.AboutMe}" />
 </td>
 </tr>



 
 <tr>
 <td>
      <apex:OutputText value="{!p.Name}" />
 </td>
 </tr>
  <tr>
 <td>
     <apex:OutputText value="{!p.Title}" />
 </td>
 </tr>
 <tr>
 <td>
      <apex:OutputText value="{!p.Phone}" />
 </td>
 </tr>
 
 
 <tr>
 <td>
      <apex:OutputText value="{!p.Email}" />
 </td>
 </tr>
 <tr><td><br/></td></tr>
  <tr><td><br/></td></tr>
 </apex:repeat>
 
 </table>
 
 </apex:pageBlock>
</apex:page>