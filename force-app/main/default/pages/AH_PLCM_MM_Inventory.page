    <script type="text/javascript">
        $(document).ready(function() {
            //Change Page Title
            window.parent.document.title = "Inventory";

            //Set in line help text height
            setHelpHeight(0);
            
            if('{!ProjectPortalStatus.IsInventoryInformationProvided}'.toLowerCase() == 'true') {
                $("input[type=text], input[type=checkbox], select").prop('disabled', true);
                //Do not display warning message for disabled controls
                unbindNavigationWarning();
            }
            else {
                $(".desc-input").each(function() {
                    var parentTr = $(this).parents('tr');
                    var flagValueAdded = false;
                    $('.num-input', parentTr).each(function() {
                        if($(this).val().trim() != '') {
                            flagValueAdded = true;    
                        }
                    });
                    
                    if($('.chk-default', parentTr).prop('checked')) {
                        flagValueAdded = true;
                    }

                    if(flagValueAdded == false && $(this).val().trim() == '') { //Disable other inputs if description is not added
                        $("input", parentTr).prop('disabled', true);
                        $(this).prop('disabled', false);
                    }
                });
                
                $(".desc-input").on("change", function() {
                    var parent = $(this).parents('tr');
                    if($(this).val().trim() != '') { //If description is not empty then enable other inputs
                        $("input", parent).prop('disabled', false);
                        $(".num-input", parent).first().focus();
                    }
                });
                
                $(".chk-default").on("change", function() { //Select only default checkbox at a time
                    var parent = $(this).parents('.div-data');
                    if($(this).is(":checked")) {
                        $(".chk-default", parent).prop('checked', false);
                        $(this).prop('checked', true);
                    }
                    else {
                        $('.chk-amadeus-default', parent).prop('checked', true);
                    }
                });

                setTimeout(function() {
                    //Display warning message while moving to other tab
                    bindNavigationWarning();
                }, 500);
            }
            
            $(".div-header").on("click", function() {
                var $data = $(this).next('.div-data');
                $data.slideToggle('slow');
                $('.div-header-icon', $(this)).toggleClass('expanded'); //Set expand/collapse icon            
                
                //Set help div height based on height of main div
                setHelpHeight(600);
            });
        });
        
        function validateForm() {
            if('{!ProjectPortalStatus.IsInventoryInformationProvided}'.toLowerCase() == 'true') {
                return true;
            }
            
            var flagError = false;
            var errorDiv = '<div class="errorMsg">This field is required</div>';
            $('.error-msg').hide();
            $('.desc-input').removeClass('error');
            $('.required-input').removeClass('error');
            $('.errorMsg').remove(); //Remove all error messages
            $('.desc-input').each(function() {
                var parentTr = $(this).parents('tr');
                if($(this).val().trim() != '') {
                    $('.required-input', parentTr).each(function() {
                        if($(this).val().trim() == '') {
                            flagError = true;
                            $(this).addClass('error');
                            $(this).parent().append(errorDiv); //Show error message
                            
                            var parent = $(this).parents('.div-data').parent();
                            $('.div-data', parent).show(); //Expand section in which error found
                            $('.div-header-icon', parent).addClass('expanded');
                        }
                    });
                }
                else {
                    var txtDescription = $(this);
                    var flagValue = $('.chk-default:checked', parentTr).length > 0;
                    $('input[type=text]', parentTr).each(function() {
                        if($(this).val().trim() != '') {
                            flagValue = true;
                            return false;
                        }
                    });
                    
                    if(flagValue) { //If other input has value then make description field require
                        flagError = true;
                        txtDescription.addClass('error');
                        txtDescription.parent().append('<div class="errorMsg">Description is required if you add measurement values</div>'); //Show error message
                    }
                }
            });

            if(flagError) {
                var pgMessage = document.getElementById('{!$Component.frmInventory.pbInventory.pgMessage}');
                $(pgMessage).hide(); //Hide success message

                $('#divErrorSummary').show();
                $('html, body').animate({ //Scroll to error summary div
                    scrollTop: $('#divErrorSummary').offset().top - 5
                }, 600);

                //Set help div height based on height of main div
                setHelpHeight(100);
                return false;
            }
            else { //Show warning message on Confirm & Submit button
                return confirm('All Function Room, Room Capacity and Inventory information will be submitted to Amadeus and you will no longer be able to change or add the details. Do you want to proceed?');
            }
        }
        
        //Display bubble help popup
        function showBubble(index) {            
            //Hide all bubble help popup
            $(".bubble-popup").hide();
            $("#divBubblePopup_" + index).show(); //Show next or prevous popup
            $('html, body').animate({ //Scroll to specific bubble
                scrollTop: $("#divBubblePopup_" + index).offset().top - 50
            }, 600);
            
            if($("#divBubblePopup_" + index).parents('.div-data:visible').length == 0) { //Open chair section if not open
                $("#divBubblePopup_" + index).parents('.data-wrapper').find('.div-header').click();
            }
        }
        
        //Click on skip bubble button
        function skipBubble() {
            if('{!ProjectBubbleStatus.IsInventoryBubbleSkipped}'.toLowerCase() != 'true') {
                //Update bubble help popup flag in NI Doc record
                updateBubbleFlag('{!ProjectId}', 'inventory');
            }
            $('.bubble-popup').hide(); //Hide Bubble Popup   
        }
    </script>
</apex:page>