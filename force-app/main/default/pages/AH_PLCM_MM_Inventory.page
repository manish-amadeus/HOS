<apex:page controller="AH_PLCM_MM_Inventory" action="{!CheckTermsAndConditionStatus}" >
    <head>
        <apex:stylesheet value="{!$Resource.AH_PLCM_MM_Style}"/>
        <apex:stylesheet value="{!$Resource.AH_PLCM_MM_Bubble_Style}" />
        <!-- Include JS -->
        <apex:includeScript value="{!$Resource.AH_PLCM_JQuery_341}" />
        <style type="text/css">
            .num-input {
                width:100px !important;
            }
            
            .chk-default {
                height: 15px;
                width: 15px;
            }
            
            .first-header {
                background-color: #f8f8f8!important;
            }
            .div-header-icon {
                display: inline-block;
                margin: 0px;
                padding: 0px;
                width: 20px;
                height: 15px;
                background-image: url('{!$Resource.AH_PLCM_MM_Expand_Collapse_Icons}');
                background-repeat: no-repeat;
                background-position: -8px -4px;
            }
            
            .bubble-popup .popup-right-corner { left: 54%; }

            .help-show .bubble-popup .popup-right-corner { left: 33%; }
            
            .bubble-popup .popup-right-corner:before { left: 90%; }
            
            body:not(.help-show) .bubble-popup .middle-bubble { left: 60%; }
            
            body:not(.help-show) .bubble-popup .middle-bubble:before { left: 10%; }
            
            .bubble-popup .margin-top-10 { margin-top: 10px !important; }
        </style>
    </head>
    <!-- Progressbar :: Start -->
    <c:AH_PLCM_MM_Progress_Bar BaseUrl="{!BaseUrl}" IsFunctionRoomInfomationProvided="{!ProjectPortalStatus.IsFunctionRoomInfomationProvided }" IsRoomCapacityInformationProvided="{!ProjectPortalStatus.IsRoomCapacityInformationProvided}"
        IsInventoryInformationProvided="{!ProjectPortalStatus.IsInventoryInformationProvided}" PerecentageCompleted="{!ProjectPortalStatus.PerecentageCompleted}" />
    <!-- Progressbar :: End -->
    <apex:form id="frmInventory">
        <apex:inputHidden value="{!inventoryObj.Id}" rendered="{!inventoryObj.Id != null}"></apex:inputHidden>
        <div style="width:100%; padding:0px;">
            <div class="main-div">
                <c:AH_PLCM_MM_Bubble_Help_Popup FlagShow="{!ProjectBubbleStatus.IsInventoryBubbleSkipped == false}" StyleClass="popup-center" Index="1" TotalBubbles="7" HeaderText="Inventory"
                   HelpText="This section will allow you to input information for the property’s furniture inventory items in order to determine the most accurate maximum capacities." />
                <c:AH_PLCM_MM_Bubble_Help_Popup StyleClass="popup-center" Index="4" TotalBubbles="7" HeaderText="Repeat"
                   HelpText="Enter any additional items in the category rows provided. If you have more than four additional items please see the help text." />
                <c:AH_PLCM_MM_Bubble_Help_Popup StyleClass="popup-center" Index="6" TotalBubbles="7" HeaderText="Continue for all Items"
                   HelpText="Repeat the entries for each of the item categories listed as needed for your property’s inventory. If you do not have any items in a specific category, move to the next category and the AHD Default will be used." />
                <c:AH_PLCM_MM_Bubble_Help_Popup StyleClass="popup-center" Index="7" TotalBubbles="7" HeaderText="Thank You"
                   HelpText="Thank you for completing these steps. When you are done, please hit Confirm & Submit at the bottom of the page to move forward." />
                <apex:pageBlock id="pbInventory">
                    <div class="page-header">
                        <table class="tbl-header" border="0" cellpadding="0" cellspacing="0">
                            <tbody>
                                <tr>
                                    <td class="td-title">
                                        <h2 class="main-title">Inventory</h2>
                                    </td>
                                    <td style="text-align: right; padding-right: 10px !important;">
                                        <input type="button" value="Take Tutorial" onclick="javacsript: showBubble(1);" class="btn-common" />
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    
                    <apex:pageMessages id="pgMessage" />
                    <div id="divErrorSummary" class="error-msg" style="text-align:center;">Please fill below required fields to proceed.</div>
                    
