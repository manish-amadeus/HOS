<apex:page standardController="NI_Project_Backlog__c" showheader="false" lightningStylesheets="true">
    
    <apex:slds rendered="true"/>
    
    <div class="slds-scope">
        
        <div style="width:800px; height:600px; position:absolute; top:0; bottom:0; left:0; right:0; margin:auto;">
            <!-- 
            <apex:image url="{!$Resource.AmadeusMBFormLogo}" width="260px" />
            <br/>
            <br/>
            -->
            <center>
                <apex:outputLabel value="Amadeus Hospitality Internal Project Request Created" style="font-family:AmadeusRegular; font-size:20pt; color:#005eb8;" />
                <br/>
                <br/>
                <apex:outputText style="font-style:normal;font-size:110%;" value="Thank you for submitting your project request." />  
                <br/>
                <br/>
                <apex:outputText style="font-style:normal;font-size:110%;" value="The Estimated Total Net Project Value (NPV) is a calculation derived from the benefits you supplied and is used to drive the numeric priority your project receives. 
                                                                                  Click the View Request button below to open your request record; 
                                                                                  here you can attach files to your project, post comments in Chatter and add additional information to the project." /> 
                <br/>
                <br/>
                <apex:outputLabel style="font-style:normal;font-size:110%;" for="link" value="Click this link to see the prioritized projects list view.  Once your project has been approved it will receive a priority." />
                <apex:outputLink id="link" value="/lightning/o/NI_Project_Backlog__c/list?filterName=00B3g000001EgU7EAK">Project Priority View</apex:outputLink>
                <br/>
                <br/>
                <br/>
                <p><b>Project Title</b>: {!NI_Project_Backlog__c.name}</p>
                <p><b>Project ID</b>: {!NI_Project_Backlog__c.Project_Backlog_ID__c}</p>
                <p><b>Organizational Impact</b>: {!NI_Project_Backlog__c.Organizational_Impact__c}</p>
                <p><b>Estimated Total Net Project Value</b>:
                <apex:outputText value=" USD{0, number, 000,000.00}">
                <apex:param value="{!NI_Project_Backlog__c.Estimated_Total_Net_Project_Value__c}" />
                </apex:outputText></p>
                <br/>
                <br/>
                <br/> 
                <br/>
                <br/>
                <br/>
              
                <apex:outputLink value="/home/home.jsp" styleClass="slds-m-top_xx-small slds-button slds-button_neutral btnSubmit" style="margin:0 auto;">Close</apex:outputLink>&nbsp;&nbsp;
                <apex:outputLink value="/{!NI_Project_Backlog__c.id}" styleClass="slds-m-top_xx-small slds-button slds-button_neutral btnSubmit" style="margin:0 auto;">View Request</apex:outputLink>
                <!-- <apex:commandLink value="" styleClass="btn" style="text-decoration:none;/>" -->
                
            </center>
            <br/>
        </div>
        
    </div>
    
