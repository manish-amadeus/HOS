    <script language="JavaScript" type="text/javascript">

        function Validate(theForm) 
        {
            
            try  
            {
                
                // FIRST NAME VALIDATION 
                if (theForm.first_name.value == "") {
                    alert("Please enter a value for the \"First Name\" field.");
                    theForm.first_name.focus();
                    return (false);
                }
                if (theForm.first_name.value.length < 1) {
                    alert("Please enter at least 1 characters in the \"First Name\" field.");
                    theForm.first_name.focus();
                    return (false);
                }
                
                // LAST NAME VALIDATION 
                if (theForm.last_name.value == "") {
                    alert("Please enter a value for the \"Last Name\" field.");
                    theForm.last_name.focus();
                    return (false);
                }            
                if (theForm.last_name.value.length < 1) {
                    alert("Please enter at least 1 characters in the \"Last Name\" field.");
                    theForm.last_name.focus();
                    return (false);
                }
                
                // REQUESTER EMAIL VALIDATION 
                if (theForm.email.value == "") {
                    alert("Please enter a value for the \"Email\" field.");
                    theForm.email.focus();
                    return (false);
                }
                if (theForm.email.value.length < 1) {
                    alert("Please enter a value for the \"Email\" field.");
                    theForm.email.focus();
                    return (false);
                }
                
                // REQUESTER PHONE VALIDATION 
                if ((theForm.phone.value == "") || (theForm.phone.value.length < 1)) 
                {
                    alert("Please enter a value for the \"Requester Phone\" field.");
                    theForm.phone.focus();
                    return (false);
                }
                
                // PROPERTY LEGAL NAME VALIDATION 
                if ((theForm.company.value == "") || (theForm.company.value.length < 1)) 
                {
                    alert("Please enter a value for the \"Property Legal Name\" field.");
                    theForm.company.focus();
                    return (false);
                }            
                
                // HOLIDEX CODE VALIDATION 
                if (document.forms[0].elements[5].value == "") {
                    alert("Please choose a value for the \"Property Holidex code\" field.");
                    document.forms[0].elements[5].focus();
                    return (false);
                }
                if (document.forms[0].elements[5].length < 1) {
                    alert("Please enter at least 1 characters in the \"Property Holidex code\" field.");
                    theForm.first_name.focus();
                    return (false);
                }
                
                // PROPERTY PHYSICAL ADDRESS VALIDATION
                if (theForm.street.value == "") {
                    alert("Please enter a value for the \"Property Physical Address\" field.");
                    theForm.street.focus();
                    return (false);
                }
                if (theForm.street.length < 1) {
                    alert("Please enter at least 1 character in the \"Property Physical Address\" field.");
                    theForm.street.focus();
                    return (false);
                }
                 // Added by Pete D - 9/18/2017            
                if (theForm.street.length > 120) {
                    alert("The maximum length for the \"Property Physical Address\" field is 120 characters.");
                    theForm.street.focus();
                    return (false);
                }
                
                // PROPERTY PHYSICAL CITY VALIDATION
                if (theForm.city.value == "") {
                    alert("Please enter a value for the \"Property Physical City\" field.");
                    theForm.city.focus();
                    return (false);
                }
                if (theForm.city.length < 1) {
                    alert("Please enter at least 1 character in the \"Property Physical City\" field.");
                    theForm.city.focus();
                    return (false);
                }
       
                // PROPERTY PHYSICAL STATE/PROVINCE
//                var pl = document.getElementById('PhysicalState');
//                var plv = pl.options[pl.selectedIndex].value;

//                if (plv == '-- US/Canada Only --')
//                {
//                    alert('Please choose a value for the \"Property Physical State Province\" field.');
//                    pl.focus();
//                    return (false);
//                }   
//                if (plv == '-- Canadian Provinces --')
//                {
//                    alert('Please choose a value for the \"Property Physical State Province\" field.');
//                    pl.focus();
//                    return (false);
//                }              

                // POSTAL CODE VALIDATION
                if (theForm.ZipCode.value == "") {
                    alert("Please enter a value for the \"Postal Code\" field.");
                    theForm.ZipCode.focus();
                    return (false);
                }
                if (theForm.ZipCode.length < 1) {
                    alert("Please enter at least 1 character in the \"Postal Code\" field.");
                    theForm.ZipCode.focus();
                    return (false);
                }
                 
                // PROPERTY PHYSICAL COUNTRY VALIDATION
                var pl1 = document.getElementById('country');
                var plv1 = pl1.options[pl1.selectedIndex].value;
                
                if (plv1 == "-- Select One --") 
                {
                    alert("Please choose a value for the \"Property Physical Country\" field.");
                    pl1.focus();
                    return (false);
                } 
                
                // PROPERTY CHANGE IN MB VALIDATION
                var pl2 = document.getElementById('00N60000001Zzxl');
                var plv2 = pl2.options[pl2.selectedIndex].value;
                
                if (plv2 == "--None--") {
                    alert("Please choose a value for the \"Property Change in MB\" field.");
                    pl2.focus();
                    return (false);
                }            
                
                // BRAND IDENTIFIER VALIDATION
                var pl3 = document.getElementById('BrandID');
                var plv3 = pl3.options[pl3.selectedIndex].value;
                
                if (plv3 == "-- Select One --") {
                    alert("Please choose a value for the \"Brand Identifier\" field.");
                    pl3.focus();
                    return (false);
                }     
                           
                
                theForm.description.value = "\nProperty Postal Code = " + theForm.ZipCode.value
                theForm.description.value = theForm.description.value + "\nDLC Name = " + theForm.dlcfirst_name.value + " " + theForm.dlclast_name.value + "\nEMail = " + theForm.dlcemail.value + "\nPhone = " + theForm.dlcphone.value;
                theForm.description.value = theForm.description.value + "\nCurrency Code = " + theForm.CurrencyCode.value + "\nUnits of Measure = " + theForm.Units1.value + " and " + theForm.Units2.value;
                theForm.description.value = theForm.description.value + "\nBrand Identifer = " + theForm.BrandID.value;
                theForm.description.value = theForm.description.value + "\nTime Zone = " + theForm.TimeZone.value;
                
                theForm.description.value = theForm.description.value + "\nLanguage = " + theForm.Language.value;
                theForm.description.value = theForm.description.value + "\neProposal Primary Language = " + theForm.eProposalPrimaryLanguage.value;
                theForm.description.value = theForm.description.value + "\neProposal Secondary Language = " + theForm.eProposalSecondaryLanguage.value;
                theForm.description.value = theForm.description.value + "\nDate Format = " + theForm.DateFormat.value;
                theForm.description.value = theForm.description.value + "\nComments = " + theForm.SubmitterComments.value;
                
                return (true);
                
            }
            catch (err)
            {
                alert('Validation javascript error occured! ' + err.message);
                return (false);
            }
    
        }

