                alert("Please enter a value for the \"Email\" field.");
                theForm.email.focus();
                return (false);
            }
            if (theForm.email.value.length < 1) {
                alert("Please enter a value for the \"Email\" field.");
                theForm.email.focus();
                return (false);
            }

            // REQUESTER PHONE VALIDATION 
            if ((theForm.phone.value == "") || (theForm.phone.value.length < 1)) 
            {
                alert("Please enter a value for the \"Requester Phone\" field.");
                theForm.phone.focus();
                return (false);
            }

