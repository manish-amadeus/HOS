    <script type="text/javascript">
        var pgMessage = document.getElementById('{!$Component.frmRoomCapacities.pbRoomCapacities.pgMessage}');
        $(document).ready(function() {
            //Change Page Title
            window.parent.document.title = "Room Capacities";

            //Set in line help text height
            setHelpHeight(0);
            $(".btn-upload").removeClass('btn');

            var flagFileUploaded = '{!lstFiles.size > 0}'.toLowerCase() == 'true';
            $('.chk-opt').prop('checked', false);
            if(flagFileUploaded || '{!objNIDoc.AH_PLCM_MM_Maximum_Capacity_Option__c}'.toLowerCase() == 'option one') {
                $('#chkOption1').prop('checked', true);
            }
            else if('{!objNIDoc.AH_PLCM_MM_Maximum_Capacity_Option__c}'.toLowerCase() == 'option two') {
                $('#chkOption2').prop('checked', true);
            }

            if('{!ProjectPortalStatus.IsInventoryInformationProvided}'.toLowerCase() == 'true') {
                $("input[type=text], input[type=checkbox], select").prop('disabled', true);
                $("#tblFileUpload").hide();
                
                if('{!lstFiles.size == 0}'.toLowerCase() == 'true') {
                    $('#divFileUpload').hide();
                }
                
                //Do not display warning message for disabled controls
                unbindNavigationWarning();
            }
            else {
                var fileUpload = document.getElementById('{!$Component.frmRoomCapacities.pbRoomCapacities.fileUpload}');
                $('.ddl-options').val($('.chk-opt:checked').val());
                $(".chk-opt").prop('disabled', flagFileUploaded);

                $(".chk-opt").on("click", function() {
                    var flagChecked = $(this).is(":checked");
                    $(".chk-opt").prop("checked", false);
                    $(this).prop("checked", flagChecked); //Make only one checkbox selected at a time
                    $(pgMessage).hide(); //Hide success message
    
                    if(flagChecked) {
                        //Remove red border
                        $(this).parents('.data-wrapper').removeClass('error');
                        
                        //Hide file upload error message
                        $(fileUpload).siblings('#errorMsg1').hide();

                        if($(this).val() == 'Option One') {
                            $('#divFileUpload').slideDown(800);
                            $('html, body').animate({ //Scroll to error summary div
                                scrollTop: 50
                            }, 900);
                        }
                        else {
                            $('#divFileUpload').slideUp(800);
                        }
                    }
                    else {
                        $('#divFileUpload').slideUp(800);
                    }
    
                    //Make drop down selection to save selected option
                    if($('.chk-opt:checked').length > 0) {
                        $('.ddl-options').val($('.chk-opt:checked').val());
                    }
                    else {
                        $('.ddl-options').val('');
                    }
    
                    //Set help div height based on height of main div
                    setHelpHeight(800);
                });

                $(fileUpload).on("change", function() {
                    if($(fileUpload).val() != '') {
                        var parent = $(this).parent('td');
                        $('.error-msg', parent).hide();
    
                        var supportedExt = ["dwg", "dxf", "pdf", "mmr", "flp", "xls", "xlsx", "doc", "docx", "zip"];
                        $(this.files).each(function(index, file) {
                            var fileName = file.name; //Get file name
                            var extension = fileName.replace(/^.*\./, ''); //Get file extension
                            var fileSize = file.size; //Get file size 
                            var fileType = file.type; //Get file type
    
                            if(!arrayContains(supportedExt, extension.toLowerCase())) {
                                $("#errorMsg2", parent).show();
                                $(fileUpload).val('');
                            }
                            else if(fileSize == 0) {
                                $("#errorMsg3", parent).show();
                                $(fileUpload).val('');
                            }
                            else if(fileSize > 25000000) {
                                $("#errorMsg4", parent).show();
                                $(fileUpload).val('');
                            }
                        });
                    }
                });
                
                $(".btn-upload").on("click", function() {
                    var parent = $(fileUpload).parent('td');
                    $('.error-msg', parent).hide();
                    if($(fileUpload).val() != '') {
                        showHideLoader(true);
                        return true;
                    }
                    else {
                        $("#errorMsg1", parent).show();
                        $(pgMessage).hide(); //Hide success message
                        return false;
                    }
                });
                    
                $("#btnReset").on("click", function() {
                    var isReset = confirm('Are you sure you want to reset all values back to Amadeus Default Standards?');
                    if(isReset) {
                        $(pgMessage).hide(); //Hide success message
                        $('.default-value').each(function(){
                            $(this).parents('tr').find('.num-input').val($(this).val())
                        });
                        
                        $('#divErrorSummary').hide(); //Hide validation message summary
                        $('.required-input').removeClass('error'); //Remove individual error messages
                        $('.errorMsg').remove(); //Remove all error messages
                    }
                });
                
                setTimeout(function() {
                    //Display warning message while moving to other tab
                    bindNavigationWarning();
                }, 500);
            }

            $(".div-header").each(function() {
                $(".header-text", this).on("click", function() {
                    var divHeader = $(this).parents('.div-header');
                    var $data = $(divHeader).next('.div-data');
                    $data.slideToggle('slow');
                    $('.div-header-icon', divHeader).toggleClass('expanded'); //Set expand/collapse icon            
                    
                    //Set help div height based on height of main div
                    setHelpHeight(600);
                });
            });
        });
        
        //Check if array contains element or not 
        function arrayContains(arr, elem) {
            var flagContains = false;
            $(arr).each(function(index, obj) {
                if(obj == elem) {
                    flagContains = true;
                    return true;
                }
            });
            return flagContains;
        }

        //Display bubble help popup
        function showBubble(index) {
            //Hide all bubble help popup
            $(".bubble-popup").hide();
            $("#divBubblePopup_" + index).show(); //Show next or prevous popup
            $('html, body').animate({ //Scroll to specific bubble
                scrollTop: $("#divBubblePopup_" + index).offset().top - 50
            }, 600);
        }
        
        function validateForm(flagConfirmAndNext) {
            if('{!ProjectPortalStatus.IsInventoryInformationProvided}'.toLowerCase() == 'true') {
                //If step is completed then return true by default
                return true;
            }
            else {
                if($('.ddl-options').val() == '') {
                    alert('Please select Option 1 or 2');
                    $(pgMessage).hide(); //Hide success message
                    $('#divOptions').addClass('error');
                    $('html, body').animate({ //Scroll to Option Div
                        scrollTop: 0
                    }, 600);
                    return false;
                }
                else {
                    $('#divOptions').removeClass('error');
                }
            
                if(flagConfirmAndNext) { //Perform validation only for Confirm & Next button
                    var flagError = false;
                    var errorDiv = '<div class="errorMsg">This field is required</div>';

                    $('#divErrorSummary').hide(); //Hide validation message summary
                    $('.required-input').removeClass('error'); //Remove individual error messages
                    $('.errorMsg').remove(); //Remove all error messages
                    $('.required-input').each(function() {
                        if($(this).val().trim() == '') {
                            flagError = true;
                            $(this).addClass('error');
                            $(this).parent().append(errorDiv); //Show error message
                            
                            var parent = $(this).parents('.div-data').parent();
                            $('.div-data', parent).show(); //Expand section in which error found
                            $('.div-header-icon', parent).addClass('expanded');
                        }
                    });
                    
                    if(flagError) {
                        $(pgMessage).hide(); //Hide success message
                        $('#divErrorSummary').show();
                        $('html, body').animate({ //Scroll to error summary div
                            scrollTop: $('#divErrorSummary').offset().top - 20
                        }, 600);
    
                        //Set help div height based on height of main div
                        setHelpHeight(100);
                        return false;
                    }
                    else {
                        return true;
                    }
                }
                else {
                    return true;
                }
            }
        }
        
        //Click on skip bubble button
        function skipBubble() {
            if('{!ProjectBubbleStatus.IsRoomCapacityBubbleSkipped}'.toLowerCase() != 'true') {
                //Update bubble help popup flag in NI Doc record
                updateBubbleFlag('{!ProjectId}', 'roomcapacities');
            }
            $('.bubble-popup').hide(); //Hide Bubble Popup   
        }
    </script>
</apex:page>