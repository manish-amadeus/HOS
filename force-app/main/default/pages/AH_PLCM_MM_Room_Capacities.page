                                    <tr>
                                        <td>
                                            Default Chair Width (in inches):
                                        </td>
                                        <td>
                                            <apex:inputField html-disabled="true" styleClass="default-value" value="{!objDefaultValues.MM_CLA_Default_Chair_Size_Width__c}" />
                                        </td>
                                        <td>
                                            <apex:inputField styleClass="num-input required-input" value="{!objUpdatedValues.MM_CLA_Default_Chair_Size_Width__c}" html-data-maxlength="3,2" />
                                        </td>
