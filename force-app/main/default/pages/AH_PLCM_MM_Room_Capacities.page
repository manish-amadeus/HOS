<apex:page controller="AH_PLCM_MM_Room_Capacities" action="{!CheckTermsAndConditionStatus}" >
    <head>
        <apex:stylesheet value="{!$Resource.AH_PLCM_MM_Style}"/>
        <apex:stylesheet value="{!$Resource.AH_PLCM_MM_Bubble_Style}" />
        <!-- Include JS -->
        <apex:includeScript value="{!$Resource.AH_PLCM_JQuery_341}" />
        <style type="text/css">
            .num-input {
                width:160px !important;
            }

            .general-text {
                width: 59% !important;
                text-align: center;
                font-weight: bold;
                padding-left: 250px;
            }
            
            .help-show .general-text {
                width: 75% !important;
                padding-left: 90px;
            }
            
            .tbl-options {
                width: 100%;
            }
            
            .tbl-options td {
                text-align: left;
                padding-bottom: 10px;
            }
            
            .tbl-options .opt-chk {
                vertical-align: top;
                padding-right: 5px;
            }
            
            .tbl-options .opt-text {
                vertical-align: middle;
                padding-top: 2px;
            }
            
            #tblFileUpload td { padding:5px; }

            .btn-upload {
                min-width: 70px !important;
                background: linear-gradient(#00ace3,#0060bc) !important;
                color: #FFFFFF !important;
                width: auto !important;
                height: 27px !important;
                cursor: pointer !important;
                font-family: Arial,Helvetica,sans-serif !important;
                font-size: 12px !important;
                font-weight: bold !important;
                border-radius: 5px !important;
                border: 1px solid #999 !important;
                box-shadow: 4px 4px 3px #999 !important;
                -webkit-box-shadow: 4px 4px 3px #999 !important;
                -moz-box-shadow: 4px 4px 3px #999 !important;
            }
            
            #divOptions.error {
                width: 99.5% !important;
                margin-bottom: 5px;
            }
            .div-header-icon {
                display: inline-block;
                margin: 0px;
                padding: 0px;
                width: 20px;
                height: 15px;
                background-image: url('{!$Resource.AH_PLCM_MM_Expand_Collapse_Icons}');
                background-repeat: no-repeat;
                background-position: -8px -4px;
            }
            
            body:not(.help-show) .bubble-popup .popup-right-corner { left: 45% !important; }

            body:not(.help-show) .bubble-popup .popup-right-corner:before { left: 90% !important; }

            .bubble-popup .margin-top-10 { margin-top: 10px !important; }
            
            .spn-red { float:right; color:red; font-size: 10px; }
            
             /* CSS apply for IE Browser */
            @media all and (-ms-high-contrast: none), (-ms-high-contrast: active) {
                /* Image Resizing */
                .img-container {
                    position: relative;
                    padding-bottom: 56.25%!important; /* 16:9 ratio */
                    /* height: 0;
                    overflow: hidden; */
                }

                .img-container img {
                    position: absolute;
                    top: 0;
                    /* left: 0; */
                    width: 100%;
                    height: 100%;                   
                }

                .btn-upload {
                    box-shadow: none!important;
                }
            }
        </style>
    </head>
    <!-- Progressbar :: Start -->
    <c:AH_PLCM_MM_Progress_Bar BaseUrl="{!BaseUrl}" IsFunctionRoomInfomationProvided="{!ProjectPortalStatus.IsFunctionRoomInfomationProvided }" IsRoomCapacityInformationProvided="{!ProjectPortalStatus.IsRoomCapacityInformationProvided}"
        IsInventoryInformationProvided="{!ProjectPortalStatus.IsInventoryInformationProvided}" PerecentageCompleted="{!ProjectPortalStatus.PerecentageCompleted}" />
    <!-- Progressbar :: End -->
    <apex:form enctype="multipart/form-data" id="frmRoomCapacities">
        <div style="width:100%; padding:0px;">
            <div class="main-div">
                <c:AH_PLCM_MM_Bubble_Help_Popup FlagShow="{!ProjectBubbleStatus.IsRoomCapacityBubbleSkipped == false}" StyleClass="popup-center" Index="1" TotalBubbles="6" HeaderText="Room Capacity Information"
                   HelpText="This page will allow you to fill in information on how the room capacities should be calculated for the various setup styles in AHD." />
                <c:AH_PLCM_MM_Bubble_Help_Popup StyleClass="popup-center" Index="6" TotalBubbles="6" HeaderText="Thank You"
                   HelpText="Thank you for completing these steps. When you are done, please hit Save & Next at the bottom of the page to move forward." />

                <apex:pageBlock id="pbRoomCapacities">
                    <div class="page-header">
                        <table class="tbl-header" border="0" cellpadding="0" cellspacing="0">
                            <tbody>
                                <tr>
                                    <td class="td-title">
                                        <h2 class="main-title">Room Capacity Information</h2>
                                    </td>
                                    <td style="text-align: right; padding-right: 10px !important;">
                                        <input type="button" value="Take Tutorial" onclick="javacsript: showBubble(1);" class="btn-common" />
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>

                    <div class="general-text">
                        Your maximum capacities can be generated one of two ways. Please place check the box below next to your chosen option; this will be reviewed in more detail during the Diagram Readiness Call.
                    </div>
                    <apex:pageMessages id="pgMessage" />
                    <div id="divOptions" class="data-wrapper" style="padding-top:10px;">
                        <table class="tbl-options" border="0" cellspacing="0" cellpadding="0">
                            <tbody>
                                <tr>
                                    <td class="opt-chk">
                                        <input type="checkbox" id="chkOption1" value="Option One" class="chk-opt" />
                                    </td>
                                    <td class="opt-text">
                                        <label for="chkOption1">
                                            <span style="font-weight:bold;">Option 1 - </span>
                                            <span>I will provide a copy of our current capacity chart which Amadeus will use to create maximum capacities within +/- a 10% tolerance of the number listed. This option is best suited for properties who do have existing capacity charts.</span>
                                        </label>
                                       
                                        <div id="divFileUpload" class="box-main" style="{!IF(objNIDoc.AH_PLCM_MM_Maximum_Capacity_Option__c == 'Option One' || lstFiles.size > 0, '', 'display:none;')}">
                                            <div class="box-header">
                                                File Upload
                                                <span class="spn-red">*File Types Allowed: .DWG, .DXF, .PDF, .MMR, .FLP, .XLS, .XLXS, .DOC, .DOCX, .ZIP</span>    
                                            </div>
                                            <div class="box-content">
                                                <table id="tblFileUpload" border="0" cellpadding="0" cellspacing="0" style="margin-bottom:10px;">
                                                    <tr>
                                                        <td style="font-size: 12px; font-weight: bold;">File:</td>
                                                        <td>
                                                            <apex:inputFile id="fileUpload" value="{!file}" filename="{!fileName}" contentType="{!contentType}" />
                                                            <div id="errorMsg1" class="error-msg">Please select a file to upload</div>
                                                            <div id="errorMsg2" class="error-msg">File type is not supported</div>
                                                            <div id="errorMsg3" class="error-msg">Blank files are not allowed</div>
                                                            <div id="errorMsg4" class="error-msg">File size is larger than 25 Mb. Please contact your Amadeus Project Manager.</div>
                                                        </td>
                                                        <td>
                                                            <apex:commandButton id="btnUpload" action="{!UploadFile}" value="Upload" styleClass="btn-upload" />
                                                        </td>
                                                    </tr>
                                                </table>
                                                <apex:actionFunction action="{!DeleteFile}" name="DeleteFile" reRender="pbFiles" oncomplete="javascript: showHideLoader(false);">
                                                    <apex:param name="FileIdToDelete" assignTo="{!FileIdToDelete}" value="" />
                                                </apex:actionFunction>
                                                <apex:pageBlockTable id="pbFiles" value="{!lstFiles}" var="objFile" rendered="{!lstFiles.size > 0}">
                                                    <apex:column headerClass="w-15" headerValue="Action">
                                                        <a href="{!objFile.DownloadUrl}" class="action-link">Download</a>
                                                        {!IF(ProjectPortalStatus.IsInventoryInformationProvided, '', ' | ')}
                                                        <apex:commandLink value="Delete" action="{!DeleteFile}" rendered="{!ProjectPortalStatus.IsInventoryInformationProvided == false}" onclick="javascript: if(confirm('Are you sure you want to delete?')) { showHideLoader(true); } else { return false; }" styleClass="action-link">
                                                            <apex:param name="FileIdToDelete" assignTo="{!FileIdToDelete}" value="{!objFile.ContentDocumentId}" />
                                                        </apex:commandLink>
                                                    </apex:column>
                                                    <apex:column headerValue="File Name" value="{!objFile.Name}" />
                                                    <apex:column headerValue="File Type" value="{!objFile.Type}" />
                                                    <apex:column headerValue="Uploaded On (GMT)" value="{!objFile.UploadedOn}" />
                                                </apex:pageBlockTable>
                                            </div>
                                        </div>
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="2">
                                        <c:AH_PLCM_MM_Bubble_Help_Popup StyleClass="popup-arrow margin-top-10" Index="2" TotalBubbles="6" HeaderText="Select Generation Method"
                                           HelpText="Begin by selecting how you wish to generate the maximum capacities from the two options outlined. If Option 1 is selected, please upload the file of your current capacity chart." />
                                    </td>
                                </tr>
                                <tr>
                                    <td class="opt-chk">
                                        <input type="checkbox" id="chkOption2" value="Option Two" class="chk-opt" />
                                    </td>
                                    <td class="opt-text">
                                        <label for="chkOption2">
                                            <span style="font-weight:bold;">Option 2 - </span>
                                            Amadeus to generate maximum capacities using room diagram dimensions, and the Inventory Worksheet for Inventory Sizes, Seating Style measurements that you have provided. Please note these maximum capacities are generated automatically based on the above. This option is best suited for pre-opening/newly renovated properties who do not have existing capacity charts.
                                        </label>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <apex:inputField value="{!objNIDoc.AH_PLCM_MM_Maximum_Capacity_Option__c}" styleClass="ddl-options" style="display:none;" />
                    <table class="tbl-header" border="0" cellpadding="0" cellspacing="0">
                            <tbody>
                                <tr>
                                    <td class="td-title" style="width: 100%;text-align: center;vertical-align: bottom;">
                                        <div id="divErrorSummary" class="error-msg"> Please fill below required fields to proceed.</div>
                                    </td>
                                    <td>
                                        <input type="button" id="btnReset" style="{!IF(ProjectPortalStatus.IsInventoryInformationProvided, 'display:none;', '')}float:right; margin-bottom:-2px!important;" value="Reset" class="btn-common" />
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    <div class="data-wrapper">
                        <table border="0" cellspacing="0" cellpadding="0" class="tbl-common" style="border-bottom: none;">
                            <thead>
                                <tr>
                                    <th style="width:35%;">&nbsp;</th>
                                    <th style="width:20%;">Description</th>
                                    <th style="width:25%;">
                                        Amadeus Default Standard Values
                                        <c:AH_PLCM_MM_Bubble_Help_Popup StyleClass="popup-arrow margin-top-10" Index="4" TotalBubbles="6" HeaderText="Default Values"
                                           HelpText="These are the industry default values when calculating max capacity. These will be used unless the Property Values column is filled in." />
                                    </th>
                                    <th style="width:20%;">
                                        Property Values
                                        <c:AH_PLCM_MM_Bubble_Help_Popup StyleClass="popup-arrow popup-right-corner margin-top-10" Index="5" TotalBubbles="6" HeaderText="Enter Values"
                                           HelpText="These values are auto-populated from the industry defaults. If your property values differ for the listed row, please indicate here by overwriting the default value. This will be used instead of the default value when the set up is created in AHD." />
                                    </th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                    
                    <!----------------------- THEATRE :: START --------------------- -->
