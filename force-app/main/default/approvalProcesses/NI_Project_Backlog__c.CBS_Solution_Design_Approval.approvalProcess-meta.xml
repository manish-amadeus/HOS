    <description>NICC-058600

Solution Design Approval Process to Corporate Business Systems Projects</description>
    <emailTemplate>AH_Business_Systems/Project_Backlog_Solution_Design_Approval_Notification</emailTemplate>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <entryCriteria>
        <booleanFilter>1 AND 2 AND 3 AND (4 OR 5)</booleanFilter>
        <criteriaItems>
            <field>NI_Project_Backlog__c.RecordType</field>
            <operation>equals</operation>
            <value>Business Systems</value>
        </criteriaItems>
        <criteriaItems>
            <field>NI_Project_Backlog__c.Requirements_Received_Date__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>NI_Project_Backlog__c.Solution_Design_Quip_Document__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>NI_Project_Backlog__c.Approval_Status__c</field>
            <operation>equals</operation>
            <value>Solution Design Rejected,Recalled</value>
        </criteriaItems>
        <criteriaItems>
            <field>NI_Project_Backlog__c.Approval_Status__c</field>
            <operation>equals</operation>
        </criteriaItems>
    </entryCriteria>
