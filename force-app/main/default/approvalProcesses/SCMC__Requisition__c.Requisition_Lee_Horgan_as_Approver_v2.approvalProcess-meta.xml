        </approvalActions>
        <assignedApprover>
            <approver>
                <name>bsintiris@newmarketinc.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <description>If Bill Sintiris is the First Level Approver and COO Approved is false, send to Bill Sintiris for approval.</description>
        <entryCriteria>
            <formula>NOT(ISBLANK(First_Level_Approver__c))
&amp;&amp;
First_Level_Approver__r.Alias = &quot;bsintiri&quot;</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>COO First Level Approver</label>
        <name>COO_First_Level_Approver</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>First_Level_Approver__c</name>
                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <description>First Level Approver does not equal Lee Horgan or Bill Sintiris</description>
        <entryCriteria>
            <formula>NOT(ISBLANK(First_Level_Approver__c))
&amp;&amp;
First_Level_Approver__r.Alias != &quot;lhorg&quot;
&amp;&amp;
First_Level_Approver__r.Alias != &quot;bsintiri&quot;</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>First Level Approver</label>
        <name>First_Level_Approver</name>
        <rejectBehavior>
            <type>RejectRequest</type>
