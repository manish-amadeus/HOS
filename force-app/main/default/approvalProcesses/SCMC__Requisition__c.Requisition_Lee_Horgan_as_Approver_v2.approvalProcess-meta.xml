                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <description>Third Level Approver does not equal Lee Horgan or Bill Sintiris</description>
        <entryCriteria>
            <formula>NOT(ISBLANK(Third_Level_Approver__c))
&amp;&amp;
Third_Level_Approver__r.Alias != &quot;lhorg&quot;
&amp;&amp;
Third_Level_Approver__r.Alias != &quot;bsintiri&quot;</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Third Level Approver</label>
        <name>Third_Level_Approver</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>mberthiaume@newmarketinc.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <description>Send the Approval to IT based on General Ledger Accounts for the Requisition Line Items</description>
        <entryCriteria>
            <formula>Requires_IT_Approval__c = True
&amp;&amp;
First_Level_Approver__r.Alias  != &quot;mbert&quot;
&amp;&amp;
First_Level_Approver__r.Manager.Alias != &quot;mbert&quot;</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Capital Hardware/Software IT Approval Step 1</label>
        <name>Capital_Hardware_Software_IT_Approval_Step_1</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>bsintiris@newmarketinc.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <description>Send the Approval to Bill Sintiris based on General Ledger Accounts for the Requisition Line Items related to Capital Hardware and Software where the total amount is over $20,000</description>
        <entryCriteria>
            <formula>Requires_IT_Approval__c = True
&amp;&amp;
Requisition_Budget__c &gt; 20000
&amp;&amp;
Bypass_IT_Approval_Step_2__c = False</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Capital Hardware/Software IT Approval Step 2 - Over $20,000</label>
        <name>Capital_Hardware_Software_IT_Approval_Step_2_Over_20_000</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>lnavarro@newmarketinc.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <description>CFO Approval required for all requisitions over $50,000</description>
        <entryCriteria>
            <criteriaItems>
                <field>SCMC__Requisition__c.Requisition_Budget__c</field>
                <operation>greaterThan</operation>
                <value>&quot;USD 50,000&quot;</value>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>CFO Approval</label>
        <name>CFO_Approval</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>lhorgan@newmarketinc.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>SCMC__Requisition__c.Requisition_Budget__c</field>
                <operation>greaterThan</operation>
                <value>&quot;USD 125,000&quot;</value>
            </criteriaItems>
            <criteriaItems>
                <field>SCMC__Requisition__c.Bypass_CEO_Approval__c</field>
                <operation>equals</operation>
                <value>False</value>
            </criteriaItems>
        </entryCriteria>
        <label>Over 125,000 Dollars (CEO)</label>
        <name>Over_125000_Dollars_CEO</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <description>NICC-023293
This approval process is executed if Lee Horgan is listed as either the First, Second or Third Level Approver</description>
    <emailTemplate>unfiled$public/Purchase_Requisition_Approval</emailTemplate>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <entryCriteria>
        <formula>SCMC__Number_of_Lines__c &gt; 0
&amp;&amp;
(
First_Level_Approver__r.Alias = &quot;lhorg&quot;
||
Second_Level_Approver__r.Alias = &quot;lhorg&quot;
||
Third_Level_Approver__r.Alias = &quot;lhorg&quot;
)</formula>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>Approve_Status_Approved</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>SCMC__SetReqComplete</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>true</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Approval_Status_Rejected</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>COO_Approved_False</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Requisition_Approval_Rejection_Notification</name>
            <type>Alert</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>Approval_Status_Submitted_For_Approval</name>
            <type>FieldUpdate</type>
        </action>
    </initialSubmissionActions>
    <label>Requisition - Lee Horgan as Approver v2</label>
    <nextAutomatedApprover>
        <useApproverFieldOfRecordOwner>false</useApproverFieldOfRecordOwner>
        <userHierarchyField>Manager</userHierarchyField>
    </nextAutomatedApprover>
    <processOrder>1</processOrder>
    <recallActions>
        <action>
            <name>Approval_Status_Recalled</name>
            <type>FieldUpdate</type>
        </action>
    </recallActions>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>false</showApprovalHistory>
</ApprovalProcess>
