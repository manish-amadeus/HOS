        <name>Second_Level_Approver</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <approvalActions>
            <action>
                <name>Controlling_Approved_Update_Yes</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>Stamp_Controlling_Decison_Date</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>Controller_Queue</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>SCMC__Requisition__c.Requisition_Budget__c</field>
                <operation>greaterOrEqual</operation>
                <value>&quot;USD 5,000&quot;</value>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Over 5k Controller Approval</label>
        <name>Over_5k_Controller_Approval</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
        <rejectionActions>
            <action>
                <name>Controlling_Approved_Update_No</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>Stamp_Controlling_Decison_Date</name>
                <type>FieldUpdate</type>
            </action>
        </rejectionActions>
