        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <approvalActions>
            <action>
                <name>COO_Approved</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>bsintiris@newmarketinc.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <description>If Bill Sintiris is the Third Level Approver and COO Approved is false, send to Bill Sintiris for approval.</description>
        <entryCriteria>
            <formula>NOT(ISBLANK(Third_Level_Approver__c))
&amp;&amp;
Third_Level_Approver__r.Alias = &quot;bsintiri&quot;
&amp;&amp;
COO_Approved__c = false</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
