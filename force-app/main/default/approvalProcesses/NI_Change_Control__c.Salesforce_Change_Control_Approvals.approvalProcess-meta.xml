    <entryCriteria>
        <formula>AND(NOT(ISPICKVAL( Approval_Status__c , &quot;Approved&quot;)), $RecordType.Name = &quot;Salesforce.com Change Request&quot;, NOT( ISBLANK(Change_Steps__c)),
NOT( ISBLANK( Rollback_Plan__c )),
NOT( ISBLANK( Test_Plan__c )))</formula>
