<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>false</active>
    <allowRecall>true</allowRecall>
    <allowedSubmitters>
        <type>allInternalUsers</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>SCMC__Requisition_Name__c</field>
        <field>Owner</field>
        <field>SCMC__Buyer__c</field>
        <field>SCMC__Instructions_to_Supplier__c</field>
        <field>SCMC__Status__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <approvalActions>
            <action>
                <name>Controlling_Approved_Update_Yes</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>Stamp_Controlling_Decison_Date</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>Controller_Queue</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <description>If Bill Sintiris is the First Level Approver and the amount is greater than USD 5,000 and Controlling hasn&apos;t approved, send to Controlling for approval before sending to Bill</description>
        <entryCriteria>
            <formula>NOT(ISBLANK(First_Level_Approver__c))
&amp;&amp;
First_Level_Approver__r.Alias = &quot;bsintiri&quot;
&amp;&amp;
USD_Requisition_Budget__c &gt;= 5000
&amp;&amp;
ISBLANK(Controlling_Approval_Decison_Date__c)</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>COO First Level Approver - Send to Controlling</label>
        <name>COO_First_Level_Approver_Send_to_Controlling</name>
        <rejectionActions>
            <action>
                <name>Controlling_Approved_Update_No</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>Stamp_Controlling_Decison_Date</name>
                <type>FieldUpdate</type>
            </action>
        </rejectionActions>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <approvalActions>
            <action>
                <name>COO_Approved</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>bsintiris@newmarketinc.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <description>If Bill Sintiris is the First Level Approver and COO Approved is false, send to Bill Sintiris for approval.</description>
        <entryCriteria>
            <formula>NOT(ISBLANK(First_Level_Approver__c))
&amp;&amp;
First_Level_Approver__r.Alias = &quot;bsintiri&quot;</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>COO First Level Approver</label>
        <name>COO_First_Level_Approver</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>First_Level_Approver__c</name>
                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <description>First Level Approver does not equal Bill Sintiris</description>
        <entryCriteria>
            <formula>NOT(ISBLANK(First_Level_Approver__c))
&amp;&amp;
First_Level_Approver__r.Alias != &quot;bsintiri&quot;</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>First Level Approver</label>
        <name>First_Level_Approver</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <approvalActions>
            <action>
                <name>Controlling_Approved_Update_Yes</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>Stamp_Controlling_Decison_Date</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>Controller_Queue</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <description>If Bill Sintiris is the Second Level Approver and the amount is greater than USD 5,000 and Controlling hasn&apos;t approved, send to Controlling for approval before sending to Bill</description>
        <entryCriteria>
            <formula>NOT(ISBLANK(Second_Level_Approver__c))
&amp;&amp;
Second_Level_Approver__r.Alias = &quot;bsintiri&quot;
&amp;&amp;
COO_Approved__c = false
&amp;&amp;
USD_Requisition_Budget__c &gt;= 5000
&amp;&amp;
ISBLANK(Controlling_Approval_Decison_Date__c)</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>COO Second Level Approver - Send to Controller Queue</label>
        <name>COO_Second_Level_Approver_Send_to_Controller_Queue</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
        <rejectionActions>
            <action>
                <name>Controlling_Approved_Update_No</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>Stamp_Controlling_Decison_Date</name>
                <type>FieldUpdate</type>
            </action>
        </rejectionActions>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <approvalActions>
            <action>
                <name>COO_Approved</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>bsintiris@newmarketinc.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <description>If Bill Sintiris is the Second Level Approver and COO Approved is false, send to Bill Sintiris for approval.</description>
        <entryCriteria>
            <formula>NOT(ISBLANK(Second_Level_Approver__c))
&amp;&amp;
Second_Level_Approver__r.Alias = &quot;bsintiri&quot;
&amp;&amp;
COO_Approved__c = false</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>COO Second Level Approver</label>
        <name>COO_Second_Level_Approver</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>Second_Level_Approver__c</name>
                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <description>Second Level Approver does not equal Bill Sintiris</description>
        <entryCriteria>
            <formula>NOT(ISBLANK(Second_Level_Approver__c))
&amp;&amp;
Second_Level_Approver__r.Alias != &quot;bsintiri&quot;</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Second Level Approver</label>
        <name>Second_Level_Approver</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <approvalActions>
            <action>
                <name>Controlling_Approved_Update_Yes</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>Stamp_Controlling_Decison_Date</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>Controller_Queue</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>SCMC__Requisition__c.USD_Requisition_Budget__c</field>
                <operation>greaterOrEqual</operation>
                <value>5000</value>
            </criteriaItems>
            <criteriaItems>
                <field>SCMC__Requisition__c.Controlling_Approval_Decison_Date__c</field>
                <operation>equals</operation>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Over 5k Controller Approval</label>
        <name>Over_5k_Controller_Approval</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
        <rejectionActions>
            <action>
                <name>Controlling_Approved_Update_No</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>Stamp_Controlling_Decison_Date</name>
                <type>FieldUpdate</type>
            </action>
        </rejectionActions>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <approvalActions>
            <action>
                <name>COO_Approved</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>bsintiris@newmarketinc.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <description>If Bill Sintiris is the Third Level Approver and COO Approved is false, send to Bill Sintiris for approval.</description>
        <entryCriteria>
            <formula>NOT(ISBLANK(Third_Level_Approver__c))
&amp;&amp;
Third_Level_Approver__r.Alias = &quot;bsintiri&quot;
&amp;&amp;
COO_Approved__c = false</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>COO Third Level Approver</label>
        <name>COO_Third_Level_Approver</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>Third_Level_Approver__c</name>
                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <description>Third Level Approver does not equal Bill Sintiris</description>
        <entryCriteria>
            <formula>NOT(ISBLANK(Third_Level_Approver__c))
&amp;&amp;
Third_Level_Approver__r.Alias != &quot;bsintiri&quot;</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Third Level Approver</label>
        <name>Third_Level_Approver</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>philipl@newmarketinc.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <description>Send the Approval to IT based on General Ledger Accounts for the Requisition Line Items
Updated per NICC-031597</description>
        <entryCriteria>
            <formula>Requires_IT_Approval__c = True
&amp;&amp;
First_Level_Approver__r.Alias  != &quot;Philipl&quot;
&amp;&amp;
First_Level_Approver__r.Manager.Alias != &quot;Philipl&quot;</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Capital Hardware/Software IT Approval Step 1</label>
        <name>Capital_Hardware_Software_IT_Approval_Step_1</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>bsintiris@newmarketinc.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <description>Send the Approval to Bill Sintiris based on General Ledger Accounts for the Requisition Line Items related to Capital Hardware and Software where the total amount is over $20,000</description>
        <entryCriteria>
            <formula>Requires_IT_Approval__c = True
&amp;&amp;
USD_Requisition_Budget__c &gt; 20000
&amp;&amp;
Bypass_IT_Approval_Step_2__c = False</formula>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Capital Hardware/Software IT Approval Step 2 - Over $20,000</label>
        <name>Capital_Hardware_Software_IT_Approval_Step_2_Over_20_000</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>bsintiris@newmarketinc.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <description>Send to Bill Sintiris if the amount is over $125,000</description>
        <entryCriteria>
            <criteriaItems>
                <field>SCMC__Requisition__c.USD_Requisition_Budget__c</field>
                <operation>greaterThan</operation>
                <value>125000</value>
            </criteriaItems>
            <criteriaItems>
                <field>SCMC__Requisition__c.COO_Approved__c</field>
                <operation>equals</operation>
                <value>False</value>
            </criteriaItems>
        </entryCriteria>
        <label>Over 125,000 Dollars (COO)</label>
        <name>Over_125_000_Dollars_COO</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <description>Updated 01/28/2019 per NICC-032619
NICC-025052
Changes to Spend Management Requisition Approval Process - Over $125,000 Send to Bill Sintiris for Approval</description>
    <emailTemplate>unfiled$public/Purchase_Requisition_Approval</emailTemplate>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <entryCriteria>
        <formula>SCMC__Number_of_Lines__c &gt; 0</formula>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>Approve_Status_Approved</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>SCMC__SetReqComplete</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>true</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Approval_Status_Rejected</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>COO_Approved_False</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Requisition_Approval_Rejection_Notification</name>
            <type>Alert</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>Approval_Status_Submitted_For_Approval</name>
            <type>FieldUpdate</type>
        </action>
    </initialSubmissionActions>
    <label>Requisition Approval Process v5</label>
    <nextAutomatedApprover>
        <useApproverFieldOfRecordOwner>false</useApproverFieldOfRecordOwner>
        <userHierarchyField>Manager</userHierarchyField>
    </nextAutomatedApprover>
    <processOrder>1</processOrder>
    <recallActions>
        <action>
            <name>Approval_Status_Recalled</name>
            <type>FieldUpdate</type>
        </action>
    </recallActions>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>false</showApprovalHistory>
</ApprovalProcess>
