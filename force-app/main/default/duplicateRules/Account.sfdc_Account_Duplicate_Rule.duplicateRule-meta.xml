<?xml version="1.0" encoding="UTF-8"?>
<DuplicateRule xmlns="http://soap.sforce.com/2006/04/metadata" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <actionOnInsert>Block</actionOnInsert>
    <actionOnUpdate>Allow</actionOnUpdate>
    <alertText>You&apos;re creating/updating a duplicate record. We recommend you use an existing record instead. If you are receiving this alert while updating an existing record, send an email to hospitality.dataintegrity@amadeus.com so the duplicate accounts can be merged</alertText>
    <description>Salesforce.com Change Rquest: NICC-017489

This rule was originally created by the Salesforce Customer Success Team as part of an Accelerator engagement</description>
    <duplicateRuleFilter>
        <booleanFilter>1 AND 2</booleanFilter>
        <duplicateRuleFilterItems>
            <field>Username</field>
            <operation>notEqual</operation>
            <value>datatherapy@newmarketinc.com</value>
            <sortOrder>1</sortOrder>
            <table>User</table>
        </duplicateRuleFilterItems>
        <duplicateRuleFilterItems>
            <field>Username</field>
            <operation>notEqual</operation>
            <value>mdugdale@newmarketinc.com</value>
            <sortOrder>2</sortOrder>
            <table>User</table>
        </duplicateRuleFilterItems>
    </duplicateRuleFilter>
    <duplicateRuleMatchRules>
        <matchRuleSObjectType>Account</matchRuleSObjectType>
        <matchingRule>Account_Matching_Rule3</matchingRule>
        <objectMapping xsi:nil="true"/>
    </duplicateRuleMatchRules>
    <isActive>true</isActive>
    <masterLabel>sfdc Account Duplicate Rule</masterLabel>
    <operationsOnUpdate>Alert</operationsOnUpdate>
    <operationsOnUpdate>Report</operationsOnUpdate>
    <securityOption>BypassSharingRules</securityOption>
    <sortOrder>2</sortOrder>
</DuplicateRule>
