<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Trans_Billing_Percent_Complete__c</fullName>
    <externalId>false</externalId>
    <formula>if( Trans_Billing_Dollars_Delivered__c &gt;  Total_Revenue_to_Recognize__c,
    100,
    if(ISBLANK(Trans_Billing_Dollars_Delivered__c), 0,
       Trans_Billing_Dollars_Delivered__c / Total_Revenue_to_Recognize__c)
 )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Percent complete calc for tranasction billing revenue, (Trans Billing Dollars / Total Revenue to Recognize)</inlineHelpText>
    <label>Trans Billing - Percent Complete</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
