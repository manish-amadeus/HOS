<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PartnerLiveDateAndQuantity</fullName>
    <active>true</active>
    <description>Live date must be entered with quantity</description>
    <errorConditionFormula>AND (
    NOT ISBLANK(Live_Date__c),
    ISBLANK(Quantity__c)
)</errorConditionFormula>
    <errorDisplayField>Live_Date__c</errorDisplayField>
    <errorMessage>Live date must be entered with quantity</errorMessage>
</ValidationRule>
