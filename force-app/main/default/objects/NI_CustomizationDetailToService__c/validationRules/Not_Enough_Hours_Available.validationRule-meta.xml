<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Not_Enough_Hours_Available</fullName>
    <active>true</active>
    <errorConditionFormula>AND (
    Actual_Hours__c &gt; Service__r.Time_Units_Balance__c,
    IsRunningTest__c = false
)</errorConditionFormula>
    <errorDisplayField>Actual_Hours__c</errorDisplayField>
    <errorMessage>Service time balance is too low and is not enough to cover the actual hours entered.</errorMessage>
</ValidationRule>
