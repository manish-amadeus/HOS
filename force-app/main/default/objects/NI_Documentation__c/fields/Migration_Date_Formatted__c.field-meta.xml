<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Migration_Date_Formatted__c</fullName>
    <description>NICC-064245</description>
    <externalId>false</externalId>
    <formula>TEXT(DAY( AH_PLCM_Migration_Date__c ))+

&quot; &quot; +CASE(MONTH(AH_PLCM_Migration_Date__c),
1, &quot;January&quot;,
2, &quot;February&quot;,
3, &quot;March&quot;,
4, &quot;April&quot;,
5, &quot;May&quot;,
6, &quot;June&quot;,
7, &quot;July&quot;,
8, &quot;August&quot;,
9, &quot;September&quot;,
10, &quot;October&quot;,
11, &quot;November&quot;,
12, &quot;December&quot;,
&quot;None&quot;)+ &quot; &quot;

+TEXT(YEAR(AH_PLCM_Migration_Date__c))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Formatted date field to be used on BE4 migration email templates.</inlineHelpText>
    <label>Migration Date Formatted</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
