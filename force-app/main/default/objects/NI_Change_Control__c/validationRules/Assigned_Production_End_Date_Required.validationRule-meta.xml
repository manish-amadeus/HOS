<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Assigned_Production_End_Date_Required</fullName>
    <active>false</active>
    <description>Assigned Production End Date Required.</description>
    <errorConditionFormula>AND(
NOT(ISBLANK(Date_Scheduled__c)),
ISBLANK(Assigned_Production_End_Date__c)
)</errorConditionFormula>
    <errorDisplayField>Assigned_Production_End_Date__c</errorDisplayField>
    <errorMessage>Please Enter Assigned Production End Date.</errorMessage>
</ValidationRule>
