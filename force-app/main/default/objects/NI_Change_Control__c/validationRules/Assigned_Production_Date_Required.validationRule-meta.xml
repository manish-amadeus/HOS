<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Assigned_Production_Date_Required</fullName>
    <active>false</active>
    <description>Assigned Production Date Required</description>
    <errorConditionFormula>AND( 
NOT(ISBLANK(Assigned_Production_End_Date__c)), 
ISBLANK(Date_Scheduled__c) 
)</errorConditionFormula>
    <errorDisplayField>Date_Scheduled__c</errorDisplayField>
    <errorMessage>Please Enter Assigned Production Date</errorMessage>
</ValidationRule>
