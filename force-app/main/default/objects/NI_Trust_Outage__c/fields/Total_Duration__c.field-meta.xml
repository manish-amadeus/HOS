<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Total_Duration__c</fullName>
    <description>Formula was updated for NICC-028950.</description>
    <externalId>false</externalId>
    <formula>VALUE(IF(OR(ISBLANK(End_Date__c), ISBLANK(End_Time_GMT__c), End_Time_GMT__c = &quot;HH:MM&quot;), &quot;&quot;, TEXT(( DATETIMEVALUE(TEXT(YEAR(End_Date__c))+&quot;-&quot; +TEXT(MONTH(End_Date__c))+&quot;-&quot;+TEXT(DAY(End_Date__c))+&quot; &quot;+End_Time_GMT__c+&quot;:00&quot;) - DATETIMEVALUE(TEXT(YEAR(Start_Date__c))+&quot;-&quot; +TEXT(MONTH(Start_Date__c))+&quot;-&quot;+TEXT(DAY(Start_Date__c))+&quot; &quot;+Start_Time_GMT__c+&quot;:00&quot;) ) * 24 * 60)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Incident End Time - Incident Start Time (in minutes)</inlineHelpText>
    <label>Total Duration</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
