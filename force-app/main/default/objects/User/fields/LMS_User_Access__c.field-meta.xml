<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LMS_User_Access__c</fullName>
    <description>Access Status for the Customer Portal Users who are connected to Amadeus Learning Universe. This field is applicable only for Customer Portal Users. So has default value of &apos;No Access&apos;. Once created in Valamis LMS they will be updated with Status &apos;LMS User Created&apos;</description>
    <externalId>false</externalId>
    <inlineHelpText>Applicable only for Customer Portal Users</inlineHelpText>
    <label>LMS User Access</label>
    <required>false</required>
    <trackFeedHistory>false</trackFeedHistory>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>No Access</fullName>
                <default>true</default>
                <label>No Access</label>
            </value>
            <value>
                <fullName>User Created in LMS</fullName>
                <default>false</default>
                <label>User Created in LMS</label>
            </value>
            <value>
                <fullName>Do not Create in LMS</fullName>
                <default>false</default>
                <label>Do not Create in LMS</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
