<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Updated_Request_Count_Correctly</fullName>
    <active>true</active>
    <description>Value can not be 2,3 or 4.</description>
    <errorConditionFormula>Or(
Updated_Request_Count_Correctly__c = 2,
Updated_Request_Count_Correctly__c = 3,
Updated_Request_Count_Correctly__c = 4,
Updated_Request_Count_Correctly__c &gt; 5
)</errorConditionFormula>
    <errorDisplayField>Updated_Request_Count_Correctly__c</errorDisplayField>
    <errorMessage>Please update your value to 0 for N/A, 1 for NO or 5 for YES.</errorMessage>
</ValidationRule>
