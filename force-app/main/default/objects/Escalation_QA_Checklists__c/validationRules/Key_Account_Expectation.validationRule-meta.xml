<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Key_Account_Expectation</fullName>
    <active>true</active>
    <description>Value can not be 2,3 or 4.</description>
    <errorConditionFormula>Or(
Key_Account_Expectation_Set__c = 2,
Key_Account_Expectation_Set__c = 3,
Key_Account_Expectation_Set__c = 4,
Key_Account_Expectation_Set__c &gt; 5
)</errorConditionFormula>
    <errorDisplayField>Key_Account_Expectation_Set__c</errorDisplayField>
    <errorMessage>Please update your value to 0 for N/A, 1 for NO or 5 for YES.</errorMessage>
</ValidationRule>
