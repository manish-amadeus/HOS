<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Focus_and_Displays_Call_Control</fullName>
    <active>true</active>
    <description>Value can not be 2,3 or 4.</description>
    <errorConditionFormula>Or(
Focus_And_Displays_Call_Control__c = 2,
Focus_And_Displays_Call_Control__c = 3,
Focus_And_Displays_Call_Control__c = 4,
Focus_And_Displays_Call_Control__c &gt; 5
)</errorConditionFormula>
    <errorDisplayField>Focus_And_Displays_Call_Control__c</errorDisplayField>
    <errorMessage>Please update your value to 0 for N/A, 1 for NO or 5 for YES.</errorMessage>
</ValidationRule>
