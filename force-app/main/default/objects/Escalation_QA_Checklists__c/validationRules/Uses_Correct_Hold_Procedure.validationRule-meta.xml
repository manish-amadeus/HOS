<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Uses_Correct_Hold_Procedure</fullName>
    <active>true</active>
    <description>Value can not be 2,3 or 4.</description>
    <errorConditionFormula>Or(
Uses_Proper_Hold_Procedures__c  = 2,
Uses_Proper_Hold_Procedures__c  = 3,
Uses_Proper_Hold_Procedures__c  = 4,
Uses_Proper_Hold_Procedures__c  &gt; 5
)</errorConditionFormula>
    <errorDisplayField>Uses_Proper_Hold_Procedures__c</errorDisplayField>
    <errorMessage>Please update your value to 0 for N/A, 1 for NO or 5 for YES.</errorMessage>
</ValidationRule>
