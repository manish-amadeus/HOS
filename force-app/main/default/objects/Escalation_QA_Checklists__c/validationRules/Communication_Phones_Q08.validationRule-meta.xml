<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Communication_Phones_Q08</fullName>
    <active>true</active>
    <description>&quot;Initial/Follow Up Call Capture -per SOP&quot; value to be between 0 and 5</description>
    <errorConditionFormula>AND (
    RecordType.DeveloperName = &quot;Customer_Care_Quality_Form&quot;, 
    OR (
        NOT(CONTAINS(&quot;0,1,2,3,4,5&quot;, TEXT(Initial_Follow_Up_Call_Capture_per_Cal__c))),
        ISNULL(Initial_Follow_Up_Call_Capture_per_Cal__c)
    )
)</errorConditionFormula>
    <errorDisplayField>Initial_Follow_Up_Call_Capture_per_Cal__c</errorDisplayField>
    <errorMessage>Please update your value to be between 0 and 5.</errorMessage>
</ValidationRule>
