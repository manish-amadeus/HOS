<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Demonstrates_Product_Knowledge</fullName>
    <active>true</active>
    <description>Value can not be 2 or 4. Project 3370</description>
    <errorConditionFormula>Or(
Demonstrates_Product_Knowledge__c = 2,
Demonstrates_Product_Knowledge__c = 4,
Demonstrates_Product_Knowledge__c &gt; 5

)</errorConditionFormula>
    <errorDisplayField>Demonstrates_Product_Knowledge__c</errorDisplayField>
    <errorMessage>Please update your value to 0 for N/A, 1 for NO, 3 for PARTIALLY COMPLETED or 5 for YES.</errorMessage>
</ValidationRule>
