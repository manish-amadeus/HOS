<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Response_Addressed_Correct_Recipients</fullName>
    <active>true</active>
    <description>Value can not be 2,3 or 4.</description>
    <errorConditionFormula>Or(
Response_Addressed_Correct_Recipients__c = 2,
Response_Addressed_Correct_Recipients__c = 3,
Response_Addressed_Correct_Recipients__c = 4,
Response_Addressed_Correct_Recipients__c &gt; 5
)</errorConditionFormula>
    <errorDisplayField>Response_Addressed_Correct_Recipients__c</errorDisplayField>
    <errorMessage>Please update your value to 0 for N/A, 1 for NO or 5 for YES.</errorMessage>
</ValidationRule>
