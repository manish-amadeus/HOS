<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Final_EMail</fullName>
    <active>true</active>
    <description>Value can not be 2,3 or 4.</description>
    <errorConditionFormula>Or(
Final_Email_All_Issues_Addressed__c = 2,
Final_Email_All_Issues_Addressed__c = 3,
Final_Email_All_Issues_Addressed__c = 4,
Final_Email_All_Issues_Addressed__c &gt; 5

)</errorConditionFormula>
    <errorDisplayField>Final_Email_All_Issues_Addressed__c</errorDisplayField>
    <errorMessage>Please update your value to 0 for N/A, 1 for NO or 5 for YES.</errorMessage>
</ValidationRule>
