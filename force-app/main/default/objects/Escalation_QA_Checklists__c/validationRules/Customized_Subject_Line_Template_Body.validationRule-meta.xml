<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Customized_Subject_Line_Template_Body</fullName>
    <active>true</active>
    <description>Value can not be 2,3 or 4.</description>
    <errorConditionFormula>Or(
 Customized_Template_If_Needed__c  = 2,
 Customized_Template_If_Needed__c  = 4,
 Customized_Template_If_Needed__c  &gt; 5

)</errorConditionFormula>
    <errorDisplayField>Customized_Template_If_Needed__c</errorDisplayField>
    <errorMessage>Please update your value to 0 for N/A, 1 for NO, 3 for PARTIALLY COMPLETED or 5 for YES</errorMessage>
</ValidationRule>
