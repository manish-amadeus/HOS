<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Case_Management_Q09</fullName>
    <active>true</active>
    <description>&quot;Directed customer to online help or LS&quot; must be 0, 1 or 5</description>
    <errorConditionFormula>AND (
    RecordType.DeveloperName = &quot;Customer_Care_Quality_Form&quot;, 
    OR (
        NOT(CONTAINS(&quot;0,1,5&quot;, TEXT(Directed_Client_To_Online_Help_Or_LS__c))),
        ISNULL(Directed_Client_To_Online_Help_Or_LS__c)
    )
)</errorConditionFormula>
    <errorDisplayField>Directed_Client_To_Online_Help_Or_LS__c</errorDisplayField>
    <errorMessage>Please update your value to: 0 for N/A, 1 for NO or 5 for YES.</errorMessage>
</ValidationRule>
