<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Tagged_Correct_Property</fullName>
    <active>true</active>
    <description>Value can not be 2,3 or 4.</description>
    <errorConditionFormula>Or(
Tagged_Correct_Property__c = 2,
Tagged_Correct_Property__c = 3,
Tagged_Correct_Property__c = 4,
Tagged_Correct_Property__c &gt; 5
)</errorConditionFormula>
    <errorDisplayField>Tagged_Correct_Property__c</errorDisplayField>
    <errorMessage>Please update your value to 0 for N/A, 1 for NO or 5 for YES.</errorMessage>
</ValidationRule>
