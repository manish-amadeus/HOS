<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NoStackRankUntilAssignedToRelease</fullName>
    <active>false</active>
    <errorConditionFormula>AND (ISBLANK(Assigned_Product_Release__c), NOT( ISNULL( Product_Release_Stack_Rank__c ) ))</errorConditionFormula>
    <errorMessage>Marquee Feature requires an assigned Product Release prior to setting Product Release Stack Rank.</errorMessage>
</ValidationRule>
