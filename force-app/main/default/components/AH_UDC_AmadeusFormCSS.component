      }

      .modal-content input[type='number'] { border: 1px solid darkgray; }
      
      .modal .modal-title; {
         font-weight: 700;
         font-size: 16px;
      }

      .modal .modal-title, .modal .close {
         display: inline-block;
      }

      body.modal-open .jexcel {
