<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Users in permission set have System Permission to View Reports &amp; Dashboards in public folder, and to run reports.  As well as to the objects tabs. NICC-046499</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>View Reports &amp; Dashboards - Platform</label>
    <license>Salesforce Platform</license>
    <tabSettings>
        <tab>standard-Dashboard</tab>
        <visibility>Visible</visibility>
    </tabSettings>
    <tabSettings>
        <tab>standard-report</tab>
        <visibility>Visible</visibility>
    </tabSettings>
    <userPermissions>
        <enabled>true</enabled>
        <name>RunReports</name>
    </userPermissions>
    <userPermissions>
        <enabled>true</enabled>
        <name>ViewPublicDashboards</name>
    </userPermissions>
    <userPermissions>
        <enabled>true</enabled>
        <name>ViewPublicReports</name>
    </userPermissions>
</PermissionSet>
